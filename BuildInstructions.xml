<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <UsingTask AssemblyFile="MSBuild.Community.Tasks.dll" TaskName="MSBuild.Community.Tasks.AssemblyInfo" />
  <Target Name="Main">
    <Error Condition="'$(BUILD_NUMBER)' == ''" Text="Must supply BUILD_NUMBER" />
    <AssemblyInfo CodeLanguage="CS" OutputFile="SolutionVersionInfo.cs"
      AssemblyVersion="$(BUILD_NUMBER)" />
  </Target>
</Project>